<?xml version="1.0" encoding="utf-8" ?>
<Page x:Class="AutoOS.Views.Settings.HomeLandingPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:dev="using:DevWinUI"
      xmlns:local="using:AutoOS"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      mc:Ignorable="d">
    <dev:AllLandingPage HeaderImage="{ThemeResource HeaderCover}"
                        HeaderOverlayImage="{ThemeResource HeaderCover}"
                        HeaderSubtitleText="{x:Bind dev:ProcessInfoHelper.Version}"
                        HeaderText="AutoOS Settings"
                        JsonNavigationService="{x:Bind local:App.Current.NavService}" />
</Page>